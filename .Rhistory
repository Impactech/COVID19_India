theme_minimal() +
geom_point(alpha = 0.3, size = 2) + geom_line(linetype = 1, alpha = 0.3) +
geom_smooth(alpha = 0.05, size=0.6, se = F) +
labs(y = "Growth rate in Percentage")  +
ylim(c(-10, 50)) +
theme(legend.position = c(0.2, 0.8) ) +
